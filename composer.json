{
    "name": "nexendrie/menu",
    "description": "Simple menu component",
    "keywords": [
      "nette", "menu", "component", "simple"
    ],
    "license": "BSD-3-Clause",
    "homepage": "https://nexendrie.gitlab.io",
    "authors": [
        {
            "name": "Jakub Konečný",
            "email": "jakub.konecny2@centrum.cz",
            "role": "Developer"
        }
    ],
    "support": {
        "source": "https://gitlab.com/nexendrie/menu/",
        "issues": "https://gitlab.com/nexendrie/menu/issues",
        "docs": "https://nexendrie.gitlab.io/menu"
    },
    "require": {
        "php": ">=7.1.0",
        "nette/application": "^2.4",
        "nette/di": "^2.4.10",
        "nette/utils": "^2.4",
        "latte/latte": "^2.4",
        "nexendrie/utils": "^1.0"
    },
    "require-dev": {
        "nexendrie/code-quality": "dev-master",
        "nexendrie/site-generator": "^1.0"
    },
    "suggest": {
        "nette/security": "To use menu item conditions related to user"
    },
    "autoload": {
        "psr-4": {
            "Nexendrie\\Menu\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nexendrie\\Menu\\": "tests/Nexendrie/Menu/"
        }
    },
    "config": {
        "classmap-authoritative": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

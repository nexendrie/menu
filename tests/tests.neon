application:
    scanDirs: false
services:
    testbench.presenterMock: Testbench\Mocks\PresenterMock
    - Nexendrie\Menu\Authorizator
    - Nexendrie\Menu\Authenticator
    - Nexendrie\Menu\Translator
extensions:
    menu: Nexendrie\Menu\DI\MenuExtension
menu:
    default:
        items:
            Test: "Test:"
            New Test: Test:new
            Structured:
                link: Test:structured
                conditions:
                    loggedIn: false
            Invisible:
                link: Test:invisible
                conditions:
                    loggedIn: true
    list:
        title: Menu
        htmlId: testMenu
        items:
            Test: "Test:"
            New Test: Test:new
    translated:
        title: Menu
        htmlId: testMenu
        translate: true
        items:
            Test: "Test:"
            New Test: Test:new
